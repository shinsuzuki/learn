set fenc=utf-8
set nobackup
set number
set noswapfile
set ignorecase
set smartcase
set wrapscan
set hlsearch
set incsearch
set cursorline
set smartindent
set wildmode=list:longest
set visualbell
set noerrorbells
set showmatch matchtime=1
set laststatus=2
set showcmd
set shiftwidth=2
set softtabstop=2
set showmatch
set title
set clipboard=unnamed,autoselect
set nowrap
set vb t_vb=
syntax enable
nmap <Esc><Esc>  :nohlsearch<CR><Esc>